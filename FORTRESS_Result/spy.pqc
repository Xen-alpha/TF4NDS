void () TeamFortress_SpyFeignDeath = {

   local float i;
   local float j;
   local entity at_spot;

   if ( self.is_feigning ) {

      at_spot = findradius (self.origin,64.000);
      i = 1.000;
      j = 0.000;
      while ( (at_spot != world) ) {

         if ( (at_spot.mdl == "progs/detpack.mdl") ) {

            j = 1.000;

         } else {

            if ( (at_spot.mdl == "progs/turrbase.mdl") ) {

               j = 1.000;

            } else {

               if ( (at_spot.mdl == "progs/turrgun.mdl") ) {

                  j = 1.000;

               } else {

                  if ( (at_spot.mdl == "progs/disp.mdl") ) {

                     j = 1.000;

                  } else {

                     if ( (((at_spot.classname == "player") && (at_spot.deadflag == 0.000)) && (self != at_spot)) ) {

                        j = 2.000;

                     }

                  }

               }

            }

         }
         if ( (j > 0.000) ) {

            i = 0.000;

         }
         at_spot = at_spot.chain;

      }
      if ( (i == 1.000) ) {

         i = (1.000 + floor ((random () * 5.000)));
         self.velocity = '0.000 0.000 0.000';
         setsize (self,'-16.000 -16.000 -24.000','16.000 16.000 32.000');
         self.view_ofs = '0.000 0.000 22.000';
         self.movetype = 3.000;
         self.is_feigning = 0.000;
         self.current_weapon = self.weapon;
         W_SetCurrentAmmo ();
         if ( (self.weapon <= 16.000) ) {

            spy_upaxe1 ();
            return ;

         }
         if ( (i == 1.000) ) {

            spy_upb1 ();

         } else {

            if ( (i == 2.000) ) {

               spy_upc1 ();

            } else {

               if ( (i == 3.000) ) {

                  spy_upd1 ();

               } else {

                  spy_upe1 ();

               }

            }

         }

      } else {

         if ( (j == 1.000) ) {

            sprint (self,2.000,"You can't get up with something\non top of you.\n");

         } else {

            sprint (self,2.000,"You can't get up while someone\nis standing on you.\n");

         }

      }

   } else {

      if ( self.hook_out ) {

         sprint (self,2.000,"You can't feign with the hook out.\n");
         return ;

      }
      if ( (!(self.flags & 512.000) && (self.waterlevel == 0.000)) ) {

         sprint (self,2.000,"You can't feign in the air.\n");
         return ;

      }
      at_spot = findradius (self.origin,64.000);
      while ( (at_spot != world) ) {

         if ( (((at_spot.classname == "player") && (self != at_spot)) && (at_spot.is_feigning == 1.000)) ) {

            sprint (self,2.000,"You can't feign on top of another spy!\n");
            return ;

         }
         at_spot = at_spot.chain;

      }
      self.is_feigning = 1.000;
      Attack_Finished (0.800);
      self.invisible_finished = 0.000;
      self.modelindex = modelindex_player;
      self.weapon = self.current_weapon;
      self.current_weapon = 0.000;
      self.weaponmodel = "";
      self.weaponframe = 0.000;
      setsize (self,'-16.000 -16.000 -24.000','16.000 16.000 -16.000');
      self.view_ofs = '0.000 0.000 4.000';
      self.movetype = 6.000;
      DeathSound ();
      self.angles_x = 0.000;
      self.angles_z = 0.000;
      if ( (self.weapon <= 16.000) ) {

         spy_die_ax1 ();
         return ;

      }
      i = (1.000 + floor ((random () * 6.000)));
      if ( (i == 1.000) ) {

         spy_diea1 ();

      } else {

         if ( (i == 2.000) ) {

            spy_dieb1 ();

         } else {

            if ( (i == 3.000) ) {

               spy_diec1 ();

            } else {

               if ( (i == 4.000) ) {

                  spy_died1 ();

               } else {

                  spy_diee1 ();

               }

            }

         }

      }

   }

};

void () TeamFortress_SpyGoUndercover = {

   local entity te;

   if ( (self.effects & (8.000 | 4.000)) ) {

      sprint (self,1.000,"You cannot disguise while glowing.\n");
      return ;

   }
   if ( (self.is_unabletospy == 1.000) ) {

      sprint (self,2.000,"You can't go undercover right now.\n");
      return ;

   }
   if ( (invis_only == 1.000) ) {

      if ( (self.is_undercover == 1.000) ) {

         self.is_undercover = 0.000;
         self.modelindex = modelindex_player;
         self.items = (self.items - (self.items & 524288.000));

      } else {

         if ( (self.is_undercover == 2.000) ) {

            sprint (self,2.000,"You stop going undercover.\n");
            self.is_undercover = 0.000;

         } else {

            if ( (self.ammo_cells > 1.000) ) {

               sprint (self,2.000,"Going undercover...\n");
               self.is_undercover = 2.000;
               te = spawn ();
               te.classname = "timer";
               te.owner = self;
               te.think = TeamFortress_SpyUndercoverThink;
               te.nextthink = (time + 4.000);

            }

         }

      }
      self.StatusRefreshTime = (time + 0.100);

   } else {

      if ( (self.is_undercover != 2.000) ) {

         self.current_menu = 12.000;
         self.menu_count = 25.000;

      } else {

         sprint (self,2.000,"You stop going undercover.\n");
         self.is_undercover = 0.000;

      }

   }

};

void (entity spy) TeamFortress_SpyCalcName = {

   local entity te;

   spy.undercover_name = string_null;
   if ( (spy.undercover_team != 0.000) ) {

      te = find (world,classname,"player");
      while ( te ) {

         if ( ((te.team_no == spy.undercover_team) && (te.skin == spy.undercover_skin)) ) {

            spy.undercover_name = te.netname;
            te = world;

         } else {

            te = find (te,classname,"player");

         }

      }
      if ( (spy.undercover_name == string_null) ) {

         te = find (world,classname,"player");
         while ( te ) {

            if ( (te.team_no == spy.undercover_team) ) {

               spy.undercover_name = te.netname;
               te = world;

            } else {

               te = find (te,classname,"player");

            }

         }

      }

   }

};

void () TeamFortress_SpyUndercoverThink = {

   local float tc;
   local string st;

   if ( (self.owner.playerclass != 8.000) ) {

      return ;

   }
   if ( (self.owner.is_undercover == 2.000) ) {

      if ( (invis_only == 1.000) ) {

         self.owner.items = (self.owner.items | 524288.000);
         self.owner.frame = 0.000;
         self.owner.modelindex = modelindex_eyes;
         self.owner.is_undercover = 1.000;

      } else {

         self.owner.immune_to_check = (time + 10.000);
         if ( (self.skin != 0.000) ) {

            sprint (self.owner,2.000,"Skin set to ");
            TeamFortress_PrintClassName (self.owner,self.skin,0.000);
            self.owner.undercover_skin = self.skin;
            TeamFortress_SetSkin (self.owner);

         }
         if ( (self.team != 0.000) ) {

            sprint (self.owner,2.000,"Colors set to Team ");
            st = ftos (self.team);
            sprint (self.owner,2.000,st);
            sprint (self.owner,2.000,"\n");
            self.owner.undercover_team = self.team;
            stuffcmd (self.owner,"color ");
            tc = (TeamFortress_TeamGetColor (self.team) - 1.000);
            st = ftos (tc);
            stuffcmd (self.owner,st);
            stuffcmd (self.owner,"\n");
            TeamFortress_SetSkin (self.owner);

         }
         TeamFortress_SpyCalcName (self.owner);
         if ( (self.owner.StatusBarSize == 0.000) ) {

            CenterPrint (self.owner,"You are now disguised.\n");

         }
         self.owner.is_undercover = 1.000;

      }

   }
   self.owner.StatusRefreshTime = (time + 0.100);
   dremove (self);

};

void (float class) TeamFortress_SpyChangeSkin = {

   local entity te;

   if ( (class == 8.000) ) {

      sprint (self,2.000,"Skin reset.");
      self.undercover_skin = 0.000;
      TeamFortress_SetSkin (self);
      if ( (self.undercover_team == 0.000) ) {

         self.is_undercover = 0.000;

      }
      return ;

   }
   sprint (self,2.000,"Going undercover...\n");
   self.is_undercover = 2.000;
   te = spawn ();
   te.classname = "timer";
   te.owner = self;
   te.think = TeamFortress_SpyUndercoverThink;
   te.nextthink = (time + 4.000);
   te.skin = class;
   TeamFortress_SetSkin (self);

};

void (float teamno) TeamFortress_SpyChangeColor = {

   local entity te;
   local string st;
   local float tc;

   if ( (teamno == self.team_no) ) {

      sprint (self,2.000,"Colors reset.");
      self.immune_to_check = (time + 10.000);
      self.undercover_team = 0.000;
      stuffcmd (self,"color ");
      tc = (TeamFortress_TeamGetColor (self.team_no) - 1.000);
      st = ftos (tc);
      stuffcmd (self,st);
      stuffcmd (self,"\n");
      if ( (self.undercover_skin == 0.000) ) {

         self.is_undercover = 0.000;

      }
      TeamFortress_SetSkin (self);
      return ;

   }
   sprint (self,2.000,"Going undercover...\n");
   self.is_undercover = 2.000;
   te = spawn ();
   te.classname = "timer";
   te.owner = self;
   te.think = TeamFortress_SpyUndercoverThink;
   te.nextthink = (time + 4.000);
   te.team = teamno;

};

void () GasGrenadeTouch = {

   sound (self,1.000,"weapons/bounce.wav",1.000,1.000);
   if ( (self.velocity == '0.000 0.000 0.000') ) {

      self.avelocity = '0.000 0.000 0.000';

   }

};

void () GasGrenadeExplode = {

   local vector tmpv;
   local entity te;
   local float pos;

   pos = pointcontents (self.origin);
   if ( (pos == -1.000) ) {

      te = spawn ();
      te.think = GasGrenadeMakeGas;
      te.nextthink = (time + 0.100);
      te.heat = 0.000;
      te.origin = self.origin;
      te.owner = self.owner;
      te.team_no = self.owner.team_no;
      te.weapon = 0.000;

   } else {

      pos = 0.000;
      while ( (pos < 10.000) ) {

         newmis = spawn ();
         setmodel (newmis,"progs/s_bubble.spr");
         setorigin (newmis,self.origin);
         newmis.movetype = 8.000;
         newmis.solid = 0.000;
         newmis.velocity = '0.000 0.000 15.000';
         newmis.velocity_z = (10.000 + (random () * 20.000));
         newmis.nextthink = (time + 0.500);
         newmis.think = bubble_bob;
         newmis.classname = "bubble";
         newmis.frame = 0.000;
         newmis.cnt = 0.000;
         setsize (newmis,'-8.000 -8.000 -8.000','8.000 8.000 8.000');
         pos = (pos + 1.000);

      }

   }
   BecomeExplosion ();

};

void () GasGrenadeMakeGas = {

   local entity te;
   local entity timer;

   self.nextthink = (time + 0.750);
   te = findradius (self.origin,200.000);
   while ( (te != world) ) {

      if ( (((te.classname == "player") && (te.deadflag == 0.000)) && (te.has_disconnected != 1.000)) ) {

         deathmsg = 24.000;
         TF_T_Damage (te,world,self.owner,10.000,(1.000 | 2.000),0.000);
         if ( (te.tfstate & 16384.000) ) {

            timer = find (world,classname,"timer");
            while ( (((timer.owner != te) || (timer.think != HallucinationTimer)) && (timer != world)) ) {

               timer = find (timer,classname,"timer");

            }
            if ( (timer != world) ) {

               timer.health = (timer.health + 25.000);
               if ( (old_grens == 1.000) ) {

                  if ( (timer.health < 100.000) ) {

                     timer.health = 100.000;

                  }
                  timer.nextthink = (time + 0.500);

               } else {

                  if ( (timer.health < 150.000) ) {

                     timer.health = 150.000;

                  }
                  timer.nextthink = (time + 0.300);

               }

            }

         } else {

            if ( (old_grens == 1.000) ) {

               stuffcmd (te,"v_cshift 50 25 50 -50\n");
               sprint (te,2.000,"Far out man!\n");

            } else {

               sprint (te,2.000,"Run for cover! They're everywhere!\n");

            }
            te.tfstate = (te.tfstate | 16384.000);
            timer = spawn ();
            if ( (old_grens == 1.000) ) {

               timer.nextthink = (time + 0.500);

            } else {

               timer.nextthink = (time + 0.300);

            }
            timer.think = HallucinationTimer;
            timer.classname = "timer";
            timer.owner = te;
            if ( (old_grens == 1.000) ) {

               timer.health = 100.000;

            } else {

               timer.health = 150.000;

            }
            timer.team_no = self.team_no;

         }

      }
      te = te.chain;

   }
   self.heat = (self.heat + 1.000);
   if ( (self.heat == 1.000) ) {

      WriteByte (0.000,23.000);
      WriteByte (0.000,4.000);
      WriteCoord (0.000,self.origin_x);
      WriteCoord (0.000,self.origin_y);
      WriteCoord (0.000,self.origin_z);
      return ;

   }
   if ( (self.heat <= 20.000) ) {

      self.weapon = (self.weapon + 1.000);
      if ( (self.weapon == 1.000) ) {

         WriteByte (0.000,23.000);
         WriteByte (0.000,10.000);
         WriteCoord (0.000,self.origin_x);
         WriteCoord (0.000,self.origin_y);
         WriteCoord (0.000,(self.origin_z - 24.000));

      } else {

         if ( (self.weapon == 2.000) ) {

            self.weapon = 0.000;

         }

      }
      return ;

   }
   dremove (self);

};

void () HallucinationTimer = {

   local entity te;
   local float tmpx;
   local float tmpy;
   local float halltype;
   local float halltype2;
   local string st;

   self.health = (self.health - 2.500);
   if ( (self.owner.playerclass == 5.000) ) {

      self.health = (self.health - 2.500);

   }
   if ( (((self.health <= 0.000) || (self.owner.deadflag != 0.000)) || (self.owner.has_disconnected == 1.000)) ) {

      self.owner.tfstate = (self.owner.tfstate - (self.owner.tfstate & 16384.000));

   }
   if ( ((self.owner.deadflag != 0.000) || (self.owner.has_disconnected == 1.000)) ) {

      dremove (self);
      return ;

   }
   if ( !(self.owner.tfstate & 16384.000) ) {

      if ( (old_grens == 1.000) ) {

         stuffcmd (self.owner,"v_cshift; wait; bf\n");

      }
      sprint (self.owner,2.000,"You feel a little better now.\n");
      dremove (self);
      return ;

