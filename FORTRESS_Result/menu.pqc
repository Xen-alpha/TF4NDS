void (float inp) Menu_Class_Input = {

   local float new_class;

   if ( ((inp > 10.000) || (inp < 1.000)) ) {

      return ;

   }
   self.impulse = (inp + 100.000);
   if ( (self.playerclass != 0.000) ) {

      new_class = 1.000;

   }
   TeamFortress_ChangeClass ();
   ResetMenu ();
   if ( ((self.playerclass != 0.000) && (new_class != 1.000)) ) {

      if ( 1.000 ) {

         self.current_menu = 6.000;

      } else {

         self.current_menu = 1.000;

      }

   }
   self.menu_displaytime = 0.000;
   self.impulse = 0.000;

};

