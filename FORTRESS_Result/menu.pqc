            }

         }

      }

   }

};

void (float inp) Menu_Team_Input = {

   if ( (inp == 5.000) ) {

      TeamFortress_TeamPutPlayerInTeam ();

   } else {

      if ( ((inp <= number_of_teams) && (inp > 0.000)) ) {

         TeamFortress_TeamSet (inp);

      } else {

         if ( ((number_of_teams == 0.000) && (inp <= 4.000)) ) {

            TeamFortress_TeamSet (inp);

         } else {

            if ( (inp == 7.000) ) {

               self.current_menu = 9.000;

            } else {

               if ( (inp == 8.000) ) {

                  PlayerObserverMode ();

               }

            }

         }

      }

   }
   ResetMenu ();
   self.impulse = 0.000;

};

void () Menu_Class = {

   local entity AD;

   AD = find (world,classname,"info_tfdetect");
   if ( AD ) {

      if ( (self.team_no == 1.000) ) {

         if ( (AD.noise1 != string_null) ) {

            CenterPrint (self,AD.noise1);
            return ;

         }

      } else {

         if ( (self.team_no == 2.000) ) {

            if ( (AD.noise2 != string_null) ) {

               CenterPrint (self,AD.noise2);
               return ;

            }

         } else {

            if ( (self.team_no == 3.000) ) {

               if ( (AD.noise3 != string_null) ) {

                  CenterPrint (self,AD.noise3);
                  return ;

               }

            } else {

